{
    "name":"removeStopFromFile",
    "description":"gets list of stop words and removes them from a list of words",
    "version":"0.0.1",
    "createdDate":"20191113",
    "model":{
        "framework":"CML",
        "tags":"common"
    },
	"input": [
    {
		  "type": "array",
      "label":"toBeCleaned"
    }
	],
	"structure": [
    {
      "operation":"getStopWords",
      "params":{"lib":"nltk"},
      "output":"stopWords"
    },
    {
      "operation":"ifNotin",
      "input": 
      {
          "arr0":"$input",
          "arr1":"$stopWords"
      },
      "output":"cleanedList"
    }
  ],
	"output": {
    "type": "array",
    "data":"$cleanedList"
	}
}
